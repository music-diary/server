{
  "name": "scale-diary-server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "deploy": "npm ci && npm run build && npm run start:prod",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "db:init": "npx prisma generate && npx prisma db push",
    "db:generate": "npx prisma generate",
    "db:migration": "npx prisma migrate dev --name init",
    "db:pull": "npx prisma db pull",
    "db:push": "npx prisma db push",
    "db:seed": "npx prisma db seed -- --environment seed",
    "db:seed:demo": "npx prisma db seed -- --environment demo",
    "db:reset": "npx prisma migrate reset"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.529.0",
    "@aws-sdk/client-ses": "^3.529.0",
    "@aws-sdk/client-sns": "^3.529.0",
    "@nestjs/cache-manager": "^2.2.2",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.4.3",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/microservices": "^10.3.9",
    "@nestjs/platform-express": "^10.4.3",
    "@nestjs/swagger": "^7.4.2",
    "@prisma/client": "^5.15.1",
    "aws4-axios": "^3.3.6",
    "axios": "^1.7.5",
    "cache-manager": "^5.7.4",
    "cache-manager-ioredis-yet": "^2.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dynamoose": "^4.0.1",
    "express": "^4.21.0",
    "ioredis": "^5.4.1",
    "joi": "^17.13.1",
    "luxon": "^3.4.4",
    "nest-winston": "^1.9.4",
    "nestjs-dynamoose": "^0.5.8",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.5",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/luxon": "^3.4.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "dynamoose-logger": "^4.0.1",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "prisma": "^5.15.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "prisma": {
    "schema": "src/database/prisma/schema.prisma",
    "seed": "ts-node src/database/prisma/seeds/index.ts"
  }
}
